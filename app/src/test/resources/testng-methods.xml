<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="SingleBrowserParallelMethods" parallel="methods" thread-count="2">

    <listeners>
        <listener class-name="app.tfm.automation.listeners.TestListener"/>
    </listeners>

    <test name="ChromeParallelMethods">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="app.tfm.automation.tests.ui.SanityTest"/>
        </classes>
    </test>

</suite>
